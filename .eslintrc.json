{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-use-before-define": 0,
        "import/no-unresolved": 0,
        // "react/jsx-indent-props": "off",
        // "react/jsx-indent" : ["error", 4, { "props": 4 }],
        // "indent": [ "error", 4],
    }
}
